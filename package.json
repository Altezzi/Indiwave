{
  "name": "indiwave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:no-check": "prisma generate && cross-env SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "import:series": "node ./scripts/import-series-folders-to-db.js",
    "db:reset": "rimraf dev.db && prisma migrate reset --force && npm run import:series"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.2",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.12.2",
    "batoto.js": "^3.1.5",
    "bcryptjs": "^3.0.2",
    "chokidar": "^4.0.3",
    "mangadex-full-api": "^6.0.0",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "prisma": "^6.16.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.19.17",
    "@types/react": "18.3.24",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^10.0.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  }
}
